<?xml version="1.0" ?>
<database version="0.1" name="HomogeneousLittleDogSmallTrain" appendToLogDir=".WordTest10" appendToDbDir=".WordTest">

  <features type="SURF">
    <!-- Common parameters -->
    <octaves      value="4"/>
    <octaveLayers value="2"/>
    <threshold    value="4000"/>

    <!-- Dynamic adjuster parameters (SURF only) -->
    <adjusterOn        value="true"/>
    <adjusterMin       value="100"/>
    <adjusterMax       value="150"/>
    <adjusterIter      value="50"/>
    <adjusterLearnRate value="2.0"/>
    <adjusterMemory    value="true"/>

    <!-- Other parameters -->
    <autoLevels  value="false"/>
    <generateLog value="true"/>
    <cache       value="true"/>
  </features>

  <histograms type="color">
    <bins       value="256"/>
    <log        value="true"/>
    <cache      value="true"/>
  </histograms>

  <dictionary type="kmeans">
    <iterations value="10"/>
    <words      value="10"/>
    <log        value="true"/>
    <cache      value="true"/>
  </dictionary>

  <classifier type="svm" input="words">
    <type       value="C_SVC"/>
    <kernelType value="POLY"/>
    <gamma      value="0.5"/>
    <degree     value="1"/>
    <log        value="true"/>
  </classifier>

  <classifier type="svm" input="color">
    <type       value="C_SVC"/>
    <kernelType value="POLY"/>
    <gamma      value="0.5"/>
    <degree     value="2"/>
    <log        value="true"/>
  </classifier>

  <display>
    <!-- A set of unique colors for displaying label information -->
    <!-- asphalt -->
    <color r="70" g="70" b="70"/>
    <!-- grass -->
    <color r="0" g="255" b="0"/>
    <!-- gravel -->
    <color r="255" g="255" b="255"/>
    <!-- soil  -->
    <color r="255" g="255" b="0"/>
    <!-- mud -->
    <color r="255" g="0" b="0"/>
    <!-- woodchip -->
    <color r="255" g="180" b="0"/>
  </display>

